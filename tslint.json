{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended",
      "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": false,
    "member-ordering": [true, {
      "order": [
        "static-field",
        "static-method",
        "instance-field",
        "constructor",
        "instance-method"
      ]
    }]
  },
  "rulesDirectory": []
}
